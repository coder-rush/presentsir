<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cf.presentsir.www.presentsir" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name=".LoginSelect"
            android:label="@string/title_activity_login_select" >
        </activity>
        <activity
            android:name=".teacherRegisterActivity"
            android:label="@string/title_activity_teacher_register" >
        </activity>
        <activity
            android:name=".teacher_login"
            android:label="@string/title_activity_teacher_login" >
        </activity>
        <activity
            android:name=".student_login"
            android:label="@string/title_activity_student_login" >
        </activity>
        <activity
            android:name=".StudentGiveAtt"
            android:label="@string/title_activity_student_give_att" >
        </activity>
        <activity
            android:name=".display_att"
            android:label="@string/title_activity_display_att" >
        </activity>
    </application>

</manifest>
